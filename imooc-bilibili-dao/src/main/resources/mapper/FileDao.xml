<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.dao.FileDao">

    <insert id="addFile">
        insert into t_file(url, type, md5, createTime) values (#{url},#{type},#{md5},#{createTime})
    </insert>

    <select id="getFileByMd5" resultType="com.imooc.domain.File">
        select * from t_file where md5=#{fileMd5}
    </select>

</mapper>